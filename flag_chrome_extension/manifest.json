{
    "name": "Flags",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Flags"
    },
    "web_accessible_resources": [
        "flag_df.csv",
        "flag_identifier.py",
        "flag_scraping.py",
        "flag_util.py"
    ]
}
